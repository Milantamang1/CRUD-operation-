@model EmployeeModel
@{
    ViewData["Title"] = "create";
    //Layout = "";
}

    <form method="post" asp-controller="Employee" asp-action="create">
    <div class="form-group">
        <label asp-for="id">Id</label>
        <input type="text" class="form-control" asp-for="id" value="@Model.id">
        <span asp-validation-for="id" class="text-danger"></span>
    </div>
        <div class="form-group">
            <label asp-for="Name">Name</label>
            <input type="text" class="form-control" asp-for="Name" value="@Model.Name">
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="Address">Address</label>
            <input type="text" class="form-control" asp-for="Address" value="@Model.Address">
            <span asp-validation-for="Address" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="Email">Email</label>
            <input type="text" class="form-control" asp-for="Email" value="@Model.Email">
            <span asp-validation-for="Email" class="text-danger"></span>
        </div>
        <div class="form-group">
           @* <select asp-for="TestId" asp-items="@ViewBag.test"></select>*@
        </div>
        <div class="form-group">
            <label asp-for="Age">Age</label>
            <input type="text" class="form-control" asp-for="Age" value="@Model.Age">
            <span asp-validation-for="Age" class="text-danger"></span>
        </div>

        <button type="submit" class="btn btn-primary">Submit</button>

    </form>
